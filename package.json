{
  "name": "proca",
  "version": "0.1.0",
  "private": true,
  "licence": "aGPL3",
  "dependencies": {
    "@alienfast/i18next-loader": "^1.1.4",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "@svgr/webpack": "5.2.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "5.1.0",
    "i18next": "^19.3.2",
    "i18next-xhr-backend": "^3.2.2",
    "page-metadata-parser": "^1.1.4",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^3.28.2",
    "react-i18next": "^11.3.3",
    "react-share": "^4.1.0",
    "react-use-queries": "^1.0.0",
    "url-parse": "^1.4.7"
  },
  "scripts": {
    "start": "npx parcel public/dev.html --global proca",
    "build": "npx parcel build public/dev.html --global proca --no-source-maps --experimental-scope-hoisting",
    "eject": "react-scripts eject",
    "wizard": "REACT_APP_WEBPACK=wizard node scripts/start.js",
    "build-wizard": "REACT_APP_WEBPACK=wizard node scripts/build.js",
    "ostart": "node scripts/start.js",
    "obuild": "node scripts/build.js",
    "otest": "node scripts/test.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-react-app": "^9.1.2",
    "yarn": "^1.21.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "nok.babel": {
    "plugins": [
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/core",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/icons",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "icons"
      ]
    ],
    "presets": [
      "react-app"
    ]
  }
}
